<UserControl x:Class="ComMonitor.MDIWindows.UserControlTCPMDIChild"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:hmvc="clr-namespace:HexMessageViewerControl;assembly=HexMessageViewerControl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <hmvc:HexMessageContainerUC MyUserControlActionObj="{Binding DataContext.UserControlAction, ElementName=mainWindow, Mode=TwoWay}" Background="White"/>
    </Grid>
</UserControl>
